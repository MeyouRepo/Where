<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Example.ForResultExample2">

    <include layout="@layout/showing_include" />

    <EditText
        android:id="@+id/EditText_On_ForResultExample"
        android:layout_width="wrap_content"
        android:layout_height="120dp"
        android:layout_below="@+id/codeTopToolbar"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="22dp"
        android:layout_marginTop="60dp"
        android:hint="请输入一个参数返回到上一活动"
        android:imeOptions="flagNoFullscreen|flagNoExtractUi"
        android:lineSpacingExtra="16dp"
        android:paddingLeft="15dp"
        android:paddingRight="15dp"
        android:textColorHint="@android:color/white"
        android:textSize="36dp" />


    <Button
        android:id="@+id/Button_Goto_On_ForResultExample"
        style="@style/showingOneTouchStyle"
        android:text="Submit" />

</RelativeLayout>