<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <include layout="@layout/showing_include" />


    <ScrollView
        android:layout_width="match_parent"
        android:layout_above="@id/Button_On_JSPNParsingExample"
        android:layout_below="@+id/codeTopToolbar"
        android:layout_height="match_parent">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/TextView_On_JSONParsingExample"
                android:layout_marginTop="60dp"
                android:layout_marginLeft="60dp"
                android:layout_marginBottom="30dp"
                android:layout_centerHorizontal="true"
                android:layout_width="wrap_content"
                android:paddingLeft="10dp"
                android:textSize="22dp"
                android:layout_height="wrap_content"/>

        </LinearLayout>

    </ScrollView>

    <Button
        android:id="@+id/Button_On_JSPNParsingExample"
        android:text="Parse"
        style="@style/showingOneTouchStyle"/>

</RelativeLayout>